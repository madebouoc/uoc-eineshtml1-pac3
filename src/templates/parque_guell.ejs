        <h3><%= parqueGuell.titulo %></h3>
        <figure>
            <picture>
                <source type="image/avif"
                        srcset="<%= parqueGuell.imagenes.intro_img %>?as=avif&width=300 300w,
                                <%= parqueGuell.imagenes.intro_img %>?as=avif&width=600 600w"
                        sizes="(max-width: 600px) 300px, (max-width: 1200px) 600px"/>
                <source type="image/webp"
                        srcset="<%= parqueGuell.imagenes.intro_img %>?as=webp&width=300 300w,
                                <%= parqueGuell.imagenes.intro_img %>?as=webp&width=600 600w"
                        sizes="(max-width: 600px) 300px, (max-width: 1200px) 600px"/>
