<%- include('partials/header') %>

<main>
    <div class="container location">
        <h3><%= casaVicens.titulo %></h3>
        <figure>
            <picture>
                <source type="image/avif" loading="lazy"
                        srcset="<%= casaVicens.imagenes.intro_img %>?as=avif&width=300 300w,
                                <%= casaVicens.imagenes.intro_img %>?as=avif&width=600 600w"
                        sizes="(max-width: 600px) 300px, (max-width: 1200px) 600px"/>
                <source type="image/webp" loading="lazy"
                        srcset="<%= casaVicens.imagenes.intro_img %>?as=webp&width=300 300w,
                                <%= casaVicens.imagenes.intro_img %>?as=webp&width=600 600w"
                        sizes="(max-width: 600px) 300px, (max-width: 1200px) 600px"/>
                <img src="<%= casaVicens.imagenes.intro_img %>?width=300"
                     srcset="<%= casaVicens.imagenes.intro_img %>?width=300 300w,
                             <%= casaVicens.imagenes.intro_img %>?width=600 600w"
                     sizes="(max-width: 600px) 300px, (max-width: 1200px) 600px"
                     alt="<%= casaVicens.titulo %>"
                     aria-label="<%= casaVicens.titulo %>"
                     loading="lazy">
            </picture>
            <figcaption><%= locations.fuente %> <a href="<%= casaVicens.imagenes.intro_link %>" target="_blank"><%= casaVicens.imagenes.intro_about %></a></figcaption>
        </figure>
        <p><%= casaVicens.descripcion.intro %></p>

        
